<!DOCTYPE html>
<html lang="en">
{% include 'links.html.twig' %}


<body class="theme-color2 light ltr">

 {% include 'header.html.twig' %}
    <!-- Log In Section Start -->
    <div class="login-section">
        <div class="materialContainer">
            <div class="box">
                <div class="login-title">
                    <h2>Belépés</h2>
                </div>
                <div class="input">
                    <label for="name">Email-cím</label>
                    <input type="text" name="name" id="name" required>
                    <span class="spin"></span>
                    <div class="valid-feedback">
                        Adja meg a korábban használt email-címét
                    </div>
                </div>

                <div class="input">
                    <label for="pass">Jelszó</label>
                    <input type="password" name="pass" id="pass">
                    <span class="spin"></span>
                </div>

                <a href="forgot.html" class="pass-forgot">Elfelejtette a jelszavát?</a>

                <div class="button login">
                    <button onclick="location.href = 'boroplug-index.html';" type="submit">
                        <span>Belépés</span>
                        <i class="fa fa-check"></i>
                    </button>
                </div>


               

                <p>Még nincs fiókja? <a href="sign-up.html" class="theme-color">Regisztrálok</a></p>

            </div>
        </div>
    </div>
    <!-- Log In Section End -->

    <!-- theme Setting Start -->

     <div class="theme-setting">
			<ul>
				<li>
					<button id="darkButton" class="btn btn-sm dark-buttton">Dark</button>
				</li>
				
				
			</ul>
		</div>
   
    <!-- theme Setting End -->

    <div class="bg-overlay"></div>

    <!-- latest jquery-->
    <script src="assets/js/jquery-3.5.1.min.js"></script>

    <!-- Bootstrap js-->
    <script src="assets/js/bootstrap/bootstrap.bundle.min.js"></script>

    <!-- feather icon js-->
    <script src="assets/js/feather/feather.min.js"></script>

    <!-- lazyload js-->
    <script src="assets/js/lazysizes.min.js"></script>

    <!-- Slick js-->
    <script src="assets/js/slick/slick.js"></script>
    <script src="assets/js/slick/slick-animation.min.js"></script>
    <script src="assets/js/slick/custom_slick.js"></script>

    <!-- Notify js-->
    <script src="assets/js/bootstrap/bootstrap-notify.min.js"></script>

    <!-- script js -->
    <script src="assets/js/theme-setting.js"></script>
    <script src="assets/js/script.js"></script>
    <script src="assets/js/home-script.js"></script>

</body>

</html>
 <div class="container">
        <h1 class="display-3 text-center mt-5">Regisztráltak</h1>
        <table class="table table-hover mt-5 text-center">
        <thead class="sticky-top table-info">
            <tr>
                <th>Név</th>
                <th>Email</th>
                <th>Jelszó</th>                
                <th>Város </th>
                <th>Cím</th>
            </tr>
        </thead>
            {% for vasarlok in loginAll %}
            
            
            <tr>
                <td>{{vasarlok.nev}}</td>
                <td>{{vasarlok.email}}</td>
                <td>{{vasarlok.jelszo}}</td>
                <td>{{vasarlok.varos_id}}</td>
                 <td>{{vasarlok.cim}}</td>
                
               
               
            </tr>
           

            {% endfor %}
        </table>
    </div>