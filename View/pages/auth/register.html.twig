<!DOCTYPE html>
<html lang="en">


	{% include 'links.html.twig' %}

	<body>

		{% include 'header.html.twig' %}

		<!-- login page start-->
		<div class="container-fluid p-0">
			<div class="row m-0">
				<div
					class="col-12 p-0">
					<!-- Sign Up Section Start -->
					{% if error %}
						<script>
							Swal.fire({icon: 'success', title: 'Jelentkezz be', text: '{{ error|e('js') }}', confirmButtonText: 'Bejelentkezés'}).then((result) => {
if (result.isConfirmed) {
window.location.href = '/login';
}
});
						</script>
					{% endif %}

					{% if error2 %}


						<script>
							Swal.fire({icon: 'error', title: 'Sikertelen regisztráció', text: '{{ error2|e('js') }}', confirmButtonText: 'Értem'}).then((result) => {
if (result.isConfirmed) {
window.location.href = '/register';
}
});
						</script>


					{% endif %}

					<div class="login-section">
						<div class="materialContainer">
							<form action="/register" method="POST">
								<div class="box">

									<div class="login-title">
										<h2>Regisztráció</h2>
									</div>

									<div class="input">
										<label for="name">Név</label>
										<input type="text" name="name" id="name" required>

									</div>


									<div class="input">
										<label for="email">Email-cím</label>
										<input type="email" name="email" id="email" required>
										<span class="spin"></span>
									</div>


									<div class="">
										<h6 class="input" for="varos">Város</h6>


										<select class="form-select" name="varos" id="varos">
											{% for varos in varosok %}
												<option value="{{varos.id}}">{{varos.varosneve}}</option>

											{% endfor %}
										</select>

									</div>


									<div class="input">
										<label for="cim">Cím</label>
										<input type="text" name="cim" id="cim" placeholder="Utca név házszám ajtó" required>
										<span class="spin"></span>
									</div>


									<div class="input">
										<label for="telefon">Mobil Telefonszám</label>
										<input type="number " pattern=".{11}" name="telefon" id="telefon" required title="11 karakter hosszuságú legyen (06)">

									</div>
									<div class="input">
										<label for="password">Jelszó:</label>
										<input type="password" name="password" id="password" required>
										<span class="spin"></span>
									</div>

									<div class="input">
										<label for="password1">Jelszó megerősítése:</label>
										<input type="password" name="password1" id="password1" required>
										<span class="spin"></span>

										<span id="password-match" style="padding: 15px;"></span>

									</div>
									<div>

										<label for="agree" style="display: inline-block; vertical-align: left;">
											<br>Elfogadom a
											<a id="tos-link" href="#">felhasználási feltételeket</a>
										</label>

										<input style="display: inline-block; vertical-align: right;" type="checkbox" name="agree" id="agree" required>

									</div>

									<script>
										document.getElementById("tos-link").addEventListener("click", function (event) {
event.preventDefault();
swal("Az oldal nem valós működésű", "A boroplug.hu csak egy fiktív tanulmányi munka, semilyen valós rendelést és fizetést nem végez, ezáltal az oldalon megjelenő termékek és azok megvásálása mindössze a működést szimulálja azonban a gyakorlati vásárlás nem történik.", "info");
});
									</script>


									<div class="button login">
										<button onclick="checkPasswords()" type="submit">
											<span>Regisztráció</span>
											<i class="fa fa-check"></i>
										</button>

									</div>


								</form>

								<p>
									<a href="/login" class="theme-color">Már van fiókod?</a>
								</p>
							</div>
						</div>
					</div>
					<!-- Sign Up Section End -->
				</div>
			</div>
		</div>
		<!-- login page End -->
		<div class="theme-setting">
			<ul>
				<li>
					<button id="darkButton" class="btn btn-sm ">
						<svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-moon-stars-fill" viewbox="0 0 16 16">
							<path d="M6 .278a.768.768 0 0 1 .08.858 7.208 7.208 0 0 0-.878 3.46c0 4.021 3.278 7.277 7.318 7.277.527 0 1.04-.055 1.533-.16a.787.787 0 0 1 .81.316.733.733 0 0 1-.031.893A8.349 8.349 0 0 1 8.344 16C3.734 16 0 12.286 0 7.71 0 4.266 2.114 1.312 5.124.06A.752.752 0 0 1 6 .278z"/>
							<path d="M10.794 3.148a.217.217 0 0 1 .412 0l.387 1.162c.173.518.579.924 1.097 1.097l1.162.387a.217.217 0 0 1 0 .412l-1.162.387a1.734 1.734 0 0 0-1.097 1.097l-.387 1.162a.217.217 0 0 1-.412 0l-.387-1.162A1.734 1.734 0 0 0 9.31 6.593l-1.162-.387a.217.217 0 0 1 0-.412l1.162-.387a1.734 1.734 0 0 0 1.097-1.097l.387-1.162zM13.863.099a.145.145 0 0 1 .274 0l.258.774c.115.346.386.617.732.732l.774.258a.145.145 0 0 1 0 .274l-.774.258a1.156 1.156 0 0 0-.732.732l-.258.774a.145.145 0 0 1-.274 0l-.258-.774a1.156 1.156 0 0 0-.732-.732l-.774-.258a.145.145 0 0 1 0-.274l.774-.258c.346-.115.617-.386.732-.732L13.863.1z"/>
						</svg>
					</button>
				</li>


			</ul>
		</div>

		<!-- latest js -->
		<script src="App/Public/template/front-end/assets/js/jquery-3.6.0.min.js"></script>

		<!-- Bootstrap js -->
		<script src="App/Public/template/front-end/assets/js/bootstrap/bootstrap.bundle.min.js"></script>

		<!-- Theme js -->
		<script src="App/Public/template/front-end/assets/js/script.js"></script>
		<!-- latest jquery-->
		<script src="App/Public/template/front-end/assets/js/jquery-3.5.1.min.js"></script>

		<!-- Bootstrap js-->
		<script src="App/Public/template/front-end/assets/js/bootstrap/bootstrap.bundle.min.js"></script>

		<!-- feather icon js-->
		<script src="App/Public/template/front-end/assets/js/feather/feather.min.js"></script>

		<!-- lazyload js-->
		<script src="App/Public/template/front-end/assets/js/lazysizes.min.js"></script>

		<!-- Slick js-->
		<script src="App/Public/template/front-end/assets/js/slick/slick.js"></script>
		<script src="App/Public/template/front-end/assets/js/slick/slick-animation.min.js"></script>
		<script src="App/Public/template/front-end/assets/js/slick/custom_slick.js"></script>

		<!-- Notify js-->
		<script src="App/Public/template/front-end/assets/js/bootstrap/bootstrap-notify.min.js"></script>

		<!-- script js -->
		<script src="App/Public/template/front-end/assets/js/theme-setting.js"></script>
		<script src="App/Public/template/front-end/assets/js/script.js"></script>
		<script src="App/Public/template/front-end/assets/js/home-script.js"></script>
	</body>

</html></body></html>
